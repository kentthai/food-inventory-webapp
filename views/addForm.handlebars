      <div id="addForm">

        <h1>Add an item</h1>
        <p>Add as much information as possible!</p>



        {{#if viewAlt}}

          <form role="form" method="post" action="/add_b" >
            {{!-- <input id="foodName" placeholder="Food Name" type="text" required name="name"/> --}}
            <h3>Food Type</h3>
            <select id="foodName" class="formBox" name="food_name">
              <option value="apple">Apple</option>
              <option value="avocado">Avocado</option>
              <option value="banana">Banana</option>
              <option value="broccoli">Broccoli</option>
              <option value="carrot">Carrot</option>
              <option value="cherry">Cherry</option>
              <option value="chili_pepper">Chill Pepper</option>
              <option value="coconut">Coconut</option>
              <option value="cookie">Cookie</option>
              <option value="corn">Corn</option>
              <option value="egg">Egg</option>
              <option value="eggplant">Eggplant</option>
              <option value="garlic">Garlic</option>
              <option value="grape">Grape</option>
              <option value="green_pepper">Green Pepper</option>
              <option value="kiwi">Kiwi</option>
              <option value="lemon">Lemon</option>
              <option value="orange">Orange</option>
              <option value="pear">Pear</option>
              <option value="peas">Peas</option>
              <option value="potato">Potato</option>
              <option value="radish">Radish</option>
              <option value="strawberry">Strawberry</option>
              <option value="tomato">Tomato</option>
              <option value="watermelon">Watermelon</option>
            </select>
            <br>

            <h3>Amount</h3>
            <input placeholder="Amount" inputmode="numeric" pattern="[0-9]*"class="formBox" name="amount" required/>
            <br>

            <h3>Expiration Date</h3>
            <input placeholder="Expiration Date" type="date" class="formBox" name="expiration" required/>
            <br>

            <button type='submit' class="buttonShort">
              <div>
                <p class="buttonText short white OkButton">Ok</p>
              </div>
            </button>

          </form>

        {{else}}

